cd /mfs1/u/viet/bayesian_dpddm

source /pkgs/anaconda3/etc/profile.d/conda.sh
conda activate dpddm

echo "Launching wandb agent task $SLURM_ARRAY_TASK_ID"
wandb agent opent03-team/bayesian_dpddm/434rgxwa